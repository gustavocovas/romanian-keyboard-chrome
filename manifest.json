{
  "manifest_version": 3,
  "name": "Romanian Virtual Keyboard",
  "description": "A simple virtual keyboard for romanian characters",
  "version": "1.0.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["keyboard.js"],
      "css": ["keyboard.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
